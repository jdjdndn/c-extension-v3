{
  "name": "vue-webpack-chrome-extension-v3",
  "version": "1.0.0",
  "description": "Webpack 4 & Vue 2 (.vue) - Chrome Extension V3",
  "scripts": {
    "watch": "npm run run-sctipt && node utils/dev.js",
    "dev": "npm run run-sctipt && cross-env https=false node utils/webserver.js",
    "devs": "npm run run-sctipt && cross-env https=true node utils/webserver.js",
    "build": "npm run run-sctipt && node utils/build.js",
    "build-zip": "npm run build && node utils/build-zip.js",
    "run-sctipt": "node script/generateBolckUrl.js"
  },
  "keywords": [
    "chrome",
    "vue",
    "webpack",
    "chrome-extension"
  ],
  "author": "Jeevan Lal",
  "license": "MIT",
  "dependencies": {
    "copy-webpack-plugin": "^6.4.1",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.3",
    "source-map-loader": "^3.0.0",
    "terser-webpack-plugin": "^4.2.3",
    "vue": "^2.6.12",
    "vue-router": "^3.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.2.2",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.4.1",
    "mini-css-extract-plugin": "^0.11.3",
    "postcss": "^7.0.32",
    "postcss-loader": "^3.0.0",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
